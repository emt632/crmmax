{
  "name": "Life Link III CRM",
  "short_name": "LL3 CRM",
  "description": "Life Link III Customer Relationship Management",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#2563eb",
  "icons": [
    {
      "src": "/icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "vcf",
          "accept": [".vcf", "text/vcard", "text/x-vcard"]
        }
      ]
    }
  }
}
